{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        },
        "debug": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/transform-runtime",
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    "universal-import",
    "react-hot-loader/babel",
    [
      "emotion", { 
        "sourceMap": true, 
        "autoLabel": true 
      }
    ]
  ]
}